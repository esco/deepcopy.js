    "eslint": "eslint",
    "lint": "npm ru eslint -- --ext .js .",
    "minify": "npm ru webpack -- --optimize-minimize --output-filename deepcopy.min.js",
    "mocha": "mocha",
    "karma": "karma",
    "test": "npm ru mocha",
    "test-node": "npm ru mocha",
    "test-browser": "npm ru karma -- start --single-run --browsers Firefox,Chrome,Safari",
    "webpack": "webpack --colors --display-error-details --progress"
  },
  "devDependencies": {
    "babel-eslint": "4.1.6",
    "babel-loader": "5.3.3",
